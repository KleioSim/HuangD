; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="HuangD.Godot"
run/main_scene="res://InitialScene/InitialScene.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/show_image=false
config/icon="res://MapScene/icon.svg"

[autoload]

Chrona_Global="*res://addons/Chrona.Engine.Godot/Global.cs"
CommandConsole="*res://addons/csharpConsole/CommandConsole.cs"
SelectEntity="*res://Utilties/SelectEntity.cs"

[dotnet]

project/assembly_name="HuangD.Godot"

[editor_plugins]

enabled=PackedStringArray("res://addons/Chrona.Engine.Godot/plugin.cfg", "res://addons/csharpConsole/plugin.cfg")
